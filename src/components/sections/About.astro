---
import Section from '@/components/Section.astro'

import { basics } from 'cv-english.json'
const { summary } = basics
---

<Section title="About me">
    <p class="page-section">
        { summary }
    </p>
</Section>

<style>
    p {
        white-space: pre-wrap;
    }

    @media print {
        .page-section {
            page-break-after: always;
        }
    }
</style>